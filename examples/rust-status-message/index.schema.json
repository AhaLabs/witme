{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "AccountId": {
      "maxLength": 64,
      "minLength": 2,
      "pattern": "^(([a-z\\d]+[-_])*[a-z\\d]+\\.)*([a-z\\d]+[-_])*[a-z\\d]+$",
      "type": "string"
    },
    "Balance": {
      "$ref": "#/definitions/U128",
      "description": "Balance is a type for storing amounts of tokens, specified in yoctoNEAR."
    },
    "Fun": {
      "additionalProperties": false,
      "properties": {
        "i": {
          "$ref": "#/definitions/i32"
        }
      },
      "required": [
        "i"
      ],
      "type": "object"
    },
    "GetFun": {
      "additionalProperties": false,
      "contractMethod": "view",
      "properties": {
        "args": {
          "additionalProperties": false,
          "type": "object"
        }
      },
      "required": [
        "args"
      ],
      "type": "object"
    },
    "GetFun__Result": {
      "$ref": "#/definitions/Fun"
    },
    "GetStatus": {
      "additionalProperties": false,
      "contractMethod": "view",
      "description": "Retreive a message for a given account id",
      "properties": {
        "args": {
          "additionalProperties": false,
          "properties": {
            "account_id": {
              "$ref": "#/definitions/AccountId"
            }
          },
          "required": [
            "account_id"
          ],
          "type": "object"
        }
      },
      "required": [
        "args"
      ],
      "type": "object"
    },
    "GetStatus__Result": {
      "type": [
        "string",
        "null"
      ]
    },
    "SetStatus": {
      "additionalProperties": false,
      "contractMethod": "change",
      "description": "Store a message for current signer account",
      "properties": {
        "args": {
          "additionalProperties": false,
          "properties": {
            "message": {
              "type": "string"
            }
          },
          "required": [
            "message"
          ],
          "type": "object"
        },
        "options": {
          "additionalProperties": false,
          "properties": {
            "attachedDeposit": {
              "$ref": "#/definitions/Balance",
              "default": "0",
              "description": "Units in yoctoNear"
            },
            "gas": {
              "default": "30000000000000",
              "description": "Units in gas",
              "pattern": "[0-9]+",
              "type": "string"
            }
          },
          "type": "object"
        }
      },
      "required": [
        "args",
        "options"
      ],
      "type": "object"
    },
    "SetStatus__Result": {
      "type": "null"
    },
    "U128": {
      "description": "String representation of a u128-bit integer",
      "pattern": "^[0-9]+$",
      "type": "string"
    },
    "i32": {
      "maximum": 2147483647,
      "minimum": -2147483648,
      "type": "integer"
    }
  }
}